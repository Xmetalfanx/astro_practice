---
import DistroData from "@layouts/linuxHQ/distrosData.astro";
import LinuxDistroTemplate from "@layouts/templates/linuxhq/linux-distro-template.astro";

const distro_name = "bunsenlabs";
const distro_title = "Bunsenlabs";

import distro_icon from "@icons/bunsenlabs.png";

const moreinfo_desktops = "Openbox";
const moreinfo_featured = "";
const moreinfo_releasecycle = "";
const moreinfo_similar_distros =
  "The Debian familys version of Archbang OR Debian with Openbox installed";
const moreinfo_target_audience =
  "Moderate to Advanced Users (for the most part)";
const moreinfo_distro_type = "";

const screenshot_desktop = "";
const screenshot_theme = "";
const screenshot_icon = "";

import screenshot_src from "@debian_ss/bunsenlabs.png";
import screenshot_thumbnail from "@debian_ss/thumbnails/bunsenlabs_tn.png";

const version_latest = "Beryllium - Released December 2022";
const versions_supported = "Beryllium";

const link_download = "https://www.bunsenlabs.org/installation.html";
const link_forum = "https://forums.bunsenlabs.org";
const link_homepage = "https://www.bunsenlabs.org";
const link_mydistro_page =
  "http://xmetal.x10.mx/linuxHQ/distros/debian/bunsenlabs.php";

const youtube_review_playlist = "PLrh2y86lvcJeeTlI3BEHVMxYbKYy74ABJ";
const youtube_tips_playlist =
  "https://www.youtube.com/playlist?list=PLrh2y86lvcJddBni43z2PSd9H0XotKdn4";
