---
import DistroData from "@layouts/linuxHQ/data/DistrosData.astro";
import LinuxDistroTemplate from "@templates/linuxhq/linux-distro-template.astro";

const distro_name = "lmde";
const distro_title = "Linux Mint Debian Edition";

import distro_icon from "@distro_icons/linuxmint.svg";

const moreinfo_desktops = "Mate, Cinnamon";
const moreinfo_featured = "";
const moreinfo_similar_distros =
  'Debian with Cinnamon (with LMDE 3, the Mate version, was dropped)OR a "Cross between Linux Mint (Ubuntu based) and Debian';
const moreinfo_target_audience = "";
const moreinfo_distro_type = "";
const moreinfo_releasecycle = "";

const screenshot_desktop = "";
const screenshot_theme = "";
const screenshot_icon = "";

const screenshot_src = "/src/pages/linuxHQ/graphics/distros/debian_based/lmde.png";
import screenshot_thumbnail from "/src/pages/linuxHQ/graphics/distros/debian_based/thumbnails/lmde-small.png";

const version_latest = "6";
const versions_supported = "5";

const link_download = "http://www.linuxmint.com/download_lmde.php";
const link_forum = "";
const link_homepage = "http://www.linuxmint.com/download_lmde.php";
const link_mydistro_page =
  "http://xmetal.x10.mx/linuxHQ/distros/debian/lmde.php";

const youtube_review_playlist = "PLrh2y86lvcJeeTlI3BEHVMxYbKYy74ABJ";
const youtube_tips_playlist =
  "https://www.youtube.com/playlist?list=PLrh2y86lvcJddBni43z2PSd9H0XotKdn4";
---

<LinuxDistroTemplate
  title="Linux Mint Debian Edition "
  meta_keywords="LMDE, Linux Mint Debian Edition, Debian-based, Linux distributions"
  meta_description="Explore Linux Mint Debian Edition (LMDE), a Debian-based Linux distribution featuring Cinnamon and Mate desktop environments. Learn about its features, target audience, and more."
>
  <DistroData
    distro_title={distro_title}
    version_latest={version_latest}
    link_homepage={link_homepage}
    link_forum={link_forum}
    link_download={link_download}
    link_mydistro_page={link_mydistro_page}
    moreinfo_desktops={moreinfo_desktops}
    moreinfo_featured={moreinfo_featured}
    moreinfo_releasecycle={moreinfo_releasecycle}
    moreinfo_similar_distros={moreinfo_similar_distros}
    moreinfo_target_audience={moreinfo_target_audience}
    moreinfo_distro_type={moreinfo_distro_type}
    distro_icon={distro_icon}
    screenshot_desktop={screenshot_desktop}
    screenshot_icon={screenshot_icon}
    screenshot_src={screenshot_src}
    screenshot_theme={screenshot_theme}
    screenshot_thumbnail={screenshot_thumbnail}
    version_latest={version_latest}
    versions_supported={versions_supported}
    youtube_review_playlist={youtube_review_playlist}
    youtube_tips_playlist={youtube_tips_playlist}
  />
</LinuxDistroTemplate>
