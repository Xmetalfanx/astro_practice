import distro_icon from "@distro_icons/deepin.png";

const moreinfo_base = "Debian";
const moreinfo_desktops = "Deepin Desktop Envirnoment";
const moreinfo_featured = "";
const moreinfo_releasecycle = "";
const moreinfo_similar_distros = "Ubuntu DDE";
