---
import DistroData from "@layouts/linuxHQ/data/DistrosData.astro";
import LinuxDistroTemplate from "@templates/linuxhq/linux-distro-template.astro";

const distro_name = "ubuntudde";
const distro_title = "Ubuntu DDE";

import distro_icon from "@distro_icons/ubuntudde.png";

const moreinfo_base = "Ubuntu Long Term Support (LTS) and non-LTS";
const moreinfo_desktops = "DDE";
const moreinfo_featured = "No";
const moreinfo_distro_type = "";
const moreinfo_releasecycle = "Follows Ubuntu's";
const moreinfo_similar_distros = "";
const moreinfo_target_audience = "";

const screenshot_desktop = "Deepin screenshot_desktop Envirnoment";
const screenshot_icon = "";
const screenshot_theme = "";

import screenshot_src from "@ubuntu_ss/ubuntudde1.png";
import screenshot_thumbnail from "@ubuntu_ss/thumbnails/ubuntudde1-small.png";

const version_latest = "23.04";
const versions_supported = "20.04 LTS 22.04 LTS 23.04";

const link_download = "https=//ubuntudde.com/link_download/";
const link_forum = "https=//discourse.ubuntudde.com/";
const link_homepage = "https=//ubuntudde.com/";
const link_mydistro_page =
  "http://xmetal.x10.mx/linuxHQ/distros/ubuntu/ubuntudde.php";

const youtube_review_playlist = "";
const youtube_tips_playlist = "";
---

<LinuxDistroTemplate
  title="Ubuntu DDE - Linux Distribution"
  meta_keywords="Ubuntu DDE, Linux, Deepin Desktop, Ubuntu-based, Linux distributions"
  meta_description="Explore Ubuntu DDE, an Ubuntu-based Linux distribution featuring the Deepin Desktop Environment. Learn about its features, target audience, and more."
>
  <DistroData
    distro_title={distro_title}
    version_latest={version_latest}
    link_homepage={link_homepage}
    link_forum={link_forum}
    linux_download={link_download}
    link_mydistro_page={link_mydistro_page}
    moreinfo_desktops={moreinfo_desktops}
    moreinfo_featured={moreinfo_featured}
    moreinfo_releasecycle={moreinfo_releasecycle}
    moreinfo_similar_distros={moreinfo_similar_distros}
    moreinfo_target_audience={moreinfo_target_audience}
    moreinfo_distro_type={moreinfo_distro_type}
    distro_icon={distro_icon}
    screenshot_desktop={screenshot_desktop}
    screenshot_icon={screenshot_icon}
    screenshot_src={screenshot_src}
    screenshot_theme={screenshot_theme}
    screenshot_thumbnail={screenshot_thumbnail}
    version_latest={version_latest}
    versions_supported={versions_supported}
    youtube_review_playlist={youtube_review_playlist}
    youtube_tips_playlist={youtube_tips_playlist}
  />
</LinuxDistroTemplate>
