---
import type { late } from "astro:schema";
import DistroData from "@layouts/linuxHQ/data/DistrosData.astro";
import LinuxDistroTemplate from "@templates/linuxhq/linux-distro-template.astro";

const distro_name = "ubuntucinnamon";
const distro_title = "Ubuntu Cinnamon";

import distro_icon from "@distro_icons/ubuntucinnamon.png";

const moreinfo_base = "Ubuntu Long Term Support (LTS) and non-LTS";
const moreinfo_desktops = "Cinnamon";
const moreinfo_featured = "yes";
const moreinfo_distro_type = "";
const moreinfo_releasecycle = "Follows Ubuntu's";
const moreinfo_similar_distros = "Linux Mint";
const moreinfo_target_audience = "";

const screenshot_desktop = "Cinnamon";
const screenshot_icon = "";
const screenshot_theme = "";

const screenshot_src =
	"/src/pages/linuxHQ/graphics/distros/ubuntu_based/ubuntu_cinnamon.png";

import screenshot_thumbnail from "/src/pages/linuxHQ/graphics/distros/ubuntu_based/thumbnails/ubuntu_cinnamon-small.png";

const version_latest = "24.04 LTS, 25.04";
const versions_supported = "24.04 LTS, 25.04";

const link_download = "https://ubuntucinnamon.org/?page_id=438";
const link_forum = "#";
const link_homepage = "https://ubuntucinnamon.org/";
const link_mydistro_page = "http://xmetal.x10.mx/linuxHQ/distros/ubuntu/";

const youtube_review_playlist = "";
const youtube_tips_playlist = "";
---

<LinuxDistroTemplate
  title="Ubuntu Cinnamon "
  meta_keywords="Ubuntu Cinnamon, Linux, Cinnamon Desktop, Ubuntu-based, Linux distributions"
  meta_description="Explore Ubuntu Cinnamon, an Ubuntu-based Linux distribution featuring the Cinnamon desktop environment. Learn about its features, target audience, and more."
>
  <DistroData
    distro_title={distro_title}
    version_latest={version_latest}
    link_homepage={link_homepage}
    link_forum={link_forum}
    link_download={link_download}
    link_mydistro_page={link_mydistro_page}
    moreinfo_desktops={moreinfo_desktops}
    moreinfo_featured={moreinfo_featured}
    moreinfo_releasecycle={moreinfo_releasecycle}
    moreinfo_similar_distros={moreinfo_similar_distros}
    moreinfo_target_audience={moreinfo_target_audience}
    moreinfo_distro_type={moreinfo_distro_type}
    distro_icon={distro_icon}
    screenshot_desktop={screenshot_desktop}
    screenshot_src={screenshot_src}
    screenshot_theme={screenshot_theme}
    screenshot_thumbnail={screenshot_thumbnail}
    version_latest={version_latest}
    versions_supported={versions_supported}
    youtube_review_playlist={youtube_review_playlist}
    youtube_tips_playlist={youtube_tips_playlist}
  />
</LinuxDistroTemplate>
