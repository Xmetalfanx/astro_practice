---
import DistroData from "@layouts/linuxHQ/DistrosData.astro";
import LinuxDistroTemplate from "@templates/linuxhq/linux-distro-template.astro";
const distro_name = "lubuntu";
const distro_title = "Lubuntu";

import distro_icon from "@icons/lubuntu.svg";

const moreinfo_base = "Ubuntu Long Term Support (LTS) and non-LTS";
const moreinfo_desktops = "LXDE LXQT";
const moreinfo_featured = "";
const moreinfo_distro_type = "";
const moreinfo_releasecycle = "";
const moreinfo_similar_distros = "";
const moreinfo_target_audience = "Any/New Users";

const screenshot_desktop = "LXQT";
const screenshot_icon = "";
const screenshot_theme = "";

import screenshot_src from "@ubuntu_ss/lubuntu.png";
import screenshot_thumbnail from "@ubuntu_ss/thumbnails/lubuntu_tn.png";

const version_latest = "23.04";
const versions_supported = "20.04 LTS 22.04 LTS 23.04";

const link_download = "https=//lubuntu.net/link_downloads/";
const link_forum = "http://ubuntulink_forums.org/";
const link_homepage = "http://lubuntu.net";
const link_mydistro_page = "http://xmetal.x10.mx/linuxHQ/distros/";

