---
import DistroData from "@layouts/linuxHQ/distrosData.astro";
import LinuxDistroTemplate from "@layouts/templates/linuxhq/linux-distro-template.astro";

const distro_name = "fedora";
const distro_title = "Fedora";

import distro_icon from "@icons/fedora.svg";

const moreinfo_desktops =
  "Gnome (Default), Various official spins with other options";
const moreinfo_featured = "yes";
const moreinfo_releasecycle = "12 Months";
const moreinfo_similar_distros = "";
const moreinfo_target_audience = "Moderate to advanced users";
const moreinfo_distro_type = "";

const screenshot_desktop = "Gnome";
const screenshot_theme = "";
const screenshot_icon = "";

import screenshot_src from "@rpm_ss/fedora.png";
import screenshot_thumbnail from "@rpm_ss/thumbnails/fedora_tn.png";

const version_latest = "38";
const versions_supported = "37, 38";

const link_download = "https://www.fedoraproject.org/";
const link_forum = "http://forums.fedoraforum.org/";
const link_homepage = "https://www.fedoraproject.org/";
const link_mydistro_page = "http://xmetal.x10.mx/linuxHQ/distros/";

const youtube_review_playlist = "PLrh2y86lvcJeLcpMeVcMwbODBKBj8wqVm";
const youtube_tips_playlist = "";
---

<LinuxDistroTemplate>
  <DistroData
    distro_title={distro_title}
    version_latest={version_latest}
    link_homepage={link_homepage}
    link_forum={link_forum}
    linux_download={link_download}
    link_mydistro_page={link_mydistro_page}
    moreinfo_desktops={moreinfo_desktops}
    moreinfo_featured={moreinfo_featured}
    moreinfo_releasecycle={moreinfo_releasecycle}
    moreinfo_similar_distros={moreinfo_similar_distros}
    moreinfo_target_audience={moreinfo_target_audience}
    moreinfo_distro_type={moreinfo_distro_type}
    distro_icon={distro_icon}
    screenshot_desktop={screenshot_desktop}
    screenshot_icon={screenshot_icon}
    screenshot_src={screenshot_src}
    screenshot_theme={screenshot_theme}
    screenshot_thumbnail={screenshot_thumbnail}
    version_latest={version_latest}
    versions_supported={versions_supported}
    youtube_review_playlist={youtube_review_playlist}
    youtube_tips_playlist={youtube_tips_playlist}
  />
</LinuxDistroTemplate>
