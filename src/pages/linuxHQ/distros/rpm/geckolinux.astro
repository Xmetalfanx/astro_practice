const distro_name = "gecko";
const distro_title = "Gecko Linux";
const family = "rpm";

import distro_icon from "@distro_icons/gecko.png";

const distro_base = "openSUSE";
const moreinfo_desktops = "Cinnamon, Mate, XFCE, Budgie, Mate, .. and more";
const moreinfo_featured = "";
const moreinfo_releasecycle = "";
const moreinfo_similar_distros = "OpenSuse";
const moreinfo_target_audience = "Moderate to Advanced Users";
const moreinfo_distro_type = "";

const screenshot_desktop = "Cinnamon";
const screenshot_theme = "";
const screenshot_icon = "";

const screenshot_src = "/src/pages/linuxHQ/graphics/distros/rpm_based/gecko.png";
import screenshot_thumbnail from "/src/pages/linuxHQ/graphics/distros/rpm_based/thumbnails/gecko-small.png";

const version_latest = "";
const versions_supported = "";

const link_download = "http://geckolinux.github.io/#download";
const link_forum = "";
