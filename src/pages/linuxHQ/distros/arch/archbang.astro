const distro_name = "archbang";
const distro_title = "Archbang";

import distro_icon from "@distro_icons/antiX.png";

const moreinfo_distro_base = "Arch Linux";
const moreinfo_desktops = "Openbox";
const moreinfo_featured = "";
const moreinfo_distro_type = "";
const moreinfo_releasecycle = "";
const moreinfo_similar_distros = "Arch";
const moreinfo_target_audience = "Moderate to Advanced Users";

const screenshot_desktop = "";
const screenshot_icon = "";
const screenshot_theme = "";

import screenshot_src from "@arch_ss/archbang.png";
import screenshot_thumbnail from "@arch_ss/thumbnails/archbang-small.png";

const version_latest = "2023.10";
const versions_supported = "unknown";

const link_download = "http://sourceforge.net/projects/archbang/files/";
const link_forum = "";
